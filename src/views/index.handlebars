<h1 class="text-center">Lista de productos est√°tica</h1>
<div class="productsCardContainer row">
    {{#each products}}
        <div class="card col-sm-2 cardProduct">
        <img class="card-img-top imgCardProducts" src="{{this.thumbnail}}" alt="Card image cap">
        <div class="card-body">
            <h5 class="card-title">{{this.title}}</h5>
            <p class="card-text">ID: {{this.id}} </p>
            <p class="card-text">{{this.description}} </p>
            <p class="card-text">Precio: {{this.price}} </p>       
            <p class="card-text">Stock: {{this.stock}} </p>   
            <p class="card-text">Code: {{this.code}} </p>
            <p class="card-text">Status: {{this.status}} </p>
        </div>
        <div>
            <button type="submit" class="btn btn-primary mb-1" id="addProductToCart">Agregar</button>
        </div>
        </div>
    {{/each}}
</div>